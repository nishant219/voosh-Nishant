{
    "version": 2,
    "builds": [
      {
        "src": "dist/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/app.js"
      }
    ],
    "env": {
      "DB_HOST": "@db_host",
      "DB_PORT": "@db_port",
      "DB_NAME": "@db_name",
      "DB_USER": "@db_user",
      "DB_PASSWORD": "@db_password"
    }
  }