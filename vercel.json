{
	"version": 2,
	"builds": [
		{
			"src": "dist/index.js",
			"use": "@vercel/node",
			"config": { "includeFiles": ["dist/**"] }
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "dist/index.js"
		}
	],
    "env": {
      "DB_HOST": "@db_host",
      "DB_PORT": "@db_port",
      "DB_NAME": "@db_name",
      "DB_USER": "@db_user",
      "DB_PASSWORD": "@db_password"
    }
  }